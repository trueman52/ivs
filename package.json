{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-create-booking-tool": "cd nova-components/CreateBookingTool && npm run dev",
        "build-create-booking-tool-prod": "cd nova-components/CreateBookingTool && npm run prod",
        "build-edit-booking-tool": "cd nova-components/EditBookingTool && npm run dev",
        "build-edit-booking-tool-prod": "cd nova-components/EditBookingTool && npm run prod",
        "build-booking-price-breakdown-resource-tool": "cd nova-components/BookingPriceBreakdownResourceTool && npm run dev",
        "build-booking-price-breakdown-resource-tool-prod": "cd nova-components/BookingPriceBreakdownResourceTool && npm run prod",
        "build-coordinator-viewer-tool": "cd nova-components/CoordinatorViewerTool && npm run dev",
        "build-coordinator-viewer-tool-prod": "cd nova-components/CoordinatorViewerTool && npm run prod",
        "build-coordinator-unit-viewer-tool": "cd nova-components/CoordinatorUnitViewerTool && npm run dev",
        "build-coordinator-unit-viewer-tool-prod": "cd nova-components/CoordinatorUnitViewerTool && npm run prod",
        "build-coordinator-booking-viewer-tool": "cd nova-components/CoordinatorBookingViewerTool && npm run dev",
        "build-coordinator-booking-viewer-tool-prod": "cd nova-components/CoordinatorBookingViewerTool && npm run prod"
    },
    "devDependencies": {
        "axios": "^0.18",
        "bootstrap": "^4.0.0",
        "cross-env": "^5.1",
        "jquery": "^3.2",
        "laravel-mix": "^4.0.7",
        "lodash": "^4.17.19",
        "popper.js": "^1.12",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.15.2",
        "sass-loader": "^7.1.0",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "bootstrap-vue": "^2.1.0",
        "moment": "^2.24.0",
        "vue-js-modal": "^1.3.31",
        "vue-multiselect": "^2.1.6",
        "vue-stripe-checkout": "^3.1.6"
    }
}
